{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "mood": {
            "enum": [
                "dark",
                "light-hearted",
                "optimistic",
                "pessimistic",
                "serious"
            ],
            "type": "string"
        },
        "style": {
            "type": "string"
        }
    },
    "required": [
        "mood",
        "style"
    ],
    "type": "object"
}

